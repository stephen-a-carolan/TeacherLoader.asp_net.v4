<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TeacherLoader.asp_net.v1</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
   


 

           
         
             
            
            <nav class="navbar navbar-expand-lg navbar-light" style= "background-color: #9370DB;">
              
  <dt class="navbar-brand" href="#">Teacher Loader </dt>
  
  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item active">
        
        <a class="nav-link text-light" asp-area="" asp-page="/index">Home</a>
      </li>
      <li class="nav-item">
        <a class="nav-link text-light" asp-area="" asp-page="/signin">Sign-In/Create Account</a>
      </li>
      <li class="nav-item">
        <a class="nav-link text-light" asp-area="" asp-page="/upload">Upload</a>
      </li>
    
    </ul>

    
  </div>
</nav>


    
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>



    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
